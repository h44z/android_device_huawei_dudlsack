# mount point	fstype    device    	[device2]
/boot		emmc		/dev/block/mmcblk0p27
/cache		ext4		/dev/block/mmcblk0p34
/data		ext4		/dev/block/mmcblk0p40
/misc		emmc		/dev/block/mmcblk0p19  
/recovery	emmc		/dev/block/mmcblk0p28
/system		ext4		/dev/block/mmcblk0p38
/cust		vfat		/dev/block/mmcblk0p39	flags=display="Cust";storage;wipeingui;backup=1
/external_sd		vfat		/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="External Storage";storage;wipeingui;removable
